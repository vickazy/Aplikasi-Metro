<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameHistoryList"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <EditText
        android:id="@+id/edtChangeUsername"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#11000000"
        android:drawableLeft="@drawable/ic_user"
        android:drawableTint="@color/fontBlack"
        android:drawablePadding="10dp"
        android:fontFamily="@font/nunito_semibold"
        android:hint="Masukkan nama pengguna baru"
        android:inputType="text"
        android:padding="10dp"
        android:textColor="@color/fontBlack"
        android:textColorHint="@color/fontGray"
        android:textSize="18sp"
        tools:ignore="HardcodedText,RtlHardcoded,UnusedAttribute" />
</LinearLayout>